{
  "name": "@jupiterone/insights-dashboards",
  "version": "2.1.0",
  "description": "JupiterOne Insights Dashboards",
  "main": "dist/insights-dashboards.cjs.js",
  "module": "dist/insights-dashboards.esm.js",
  "browser": "dist/insights-dashboards.umd.js",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
