{
  "name": "@jupiterone/insights-dashboards",
  "version": "2.0.1",
  "description": "JupiterOne Insights Dashboards",
  "main": "insights-dashboards.cjs.js",
  "module": "insights-dashboards.esm.js",
  "browser": "insights-dashboards.umd.js",
  "files": [
    "boards.json",
    "insights-dashboards.cjs.js",
    "insights-dashboards.esm.js",
    "insights-dashboards.umd.js"
  ],
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "postbuild": "node ./bin/buildJSON.js --experimental-modules; cp yarn.lock package.json ./dist",
    "deploy": "jupiterone-manual-deploy -t jupiterone-dev -a apply",
    "destroy": "jupiterone-manual-deploy -t jupiterone-dev -a destroy",
    "plan": "jupiterone-manual-deploy -t jupiterone-dev -a plan",
    "typecheck": "tsc -b"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "rollup": "^2.41.2"
  },
  "dependencies": {
    "@jupiterone/dev-tools": "^11.3.1",
    "typescript": "^4.8.4"
  }
}
