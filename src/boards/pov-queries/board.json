    {
      "description": "insert name of application when prompted",
      "id": "821f19b4-824e-4627-a467-4b318670d688",
      "title": "Which employees have installed ______ application on their device?",
      "type": "table",
      "config": {
        "queries": [
          {
            "name": "query1",
            "query": "FIND Person as p \nTHAT OWNS Device as d \nTHAT INSTALLED Application with name ~= '{{application name}}' as a\nRETURN\np.name, d.name, a.name"
          }
        ]
      }
    },
    {
      "id": "1d80e785-f22a-4742-b981-1456e8df359a",
      "title": "EC2 (active) instances accessible from the Internet via unrestricted Security Groups + public load balancers + public subnets",
      "type": "graph",
      "config": {
        "queries": [
          {
            "name": "query1",
            "query": "Find Gateway with public = true as g\nthat has aws_vpc as v\nthat contains aws_subnet with public = true as s\nthat HAS aws_instance with active = true as i\nthat PROTECTS aws_security_group as sg\nthat ALLOWS as rule Internet\nreturn tree"
          }
        ]
      }
    },
    {
      "id": "39edd73d-a97d-4209-a260-56e6b031f291",
      "title": "Which employees have access to ____ SaaS application?",
      "type": "table",
      "config": {
        "queries": [
          {
            "name": "query1",
            "query": "find Person as p \nthat is User as u \nthat (assigned | has) (Application | UserGroup) with name ~= '{{application or group name}}' as a \nreturn\np.name, u._type, a._type, a.name"
          }
        ]
      }
    }
